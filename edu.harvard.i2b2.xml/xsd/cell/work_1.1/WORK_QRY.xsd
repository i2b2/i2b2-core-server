<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:work="http://www.i2b2.org/xsd/cell/work/1.1/"
    targetNamespace="http://www.i2b2.org/xsd/cell/work/1.1/">
    
<!--    <xs:element name="examples">
        <xs:complexType>
            <xs:sequence maxOccurs="unbounded">
                <xs:element name="message_body">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:any namespace="##targetNamespace"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>   
            </xs:sequence>
        </xs:complexType>
    </xs:element>
-->
    <xs:element name="get_children" type="work:get_childrenType"/>
    <xs:element name="get_folders_by_project" type="work:get_returnType"/>
    <xs:element name="get_folders_by_userId" type="work:get_returnType"/>
    <xs:element name="delete_child" type="work:delete_childType"/>
    <xs:element name="move_child" type="work:childType"/>
    <xs:element name="rename_child" type="work:rename_childType"/>
    <xs:element name="annotate_child" type="work:annotate_childType"/>
    <xs:element name="export_child" type="work:export_childType"/>
    <xs:element name="get_name_info" type="work:findBy_childType" />
    <xs:element name="set_protected_access" type="work:protectedType" />
	<xs:element name="get_all_dblookups" type="work:get_dblookupsType"/><!-- swc20160518 added --> 
	<xs:element name="get_dblookup" type="work:get_dblookupType"/><!-- swc20160518 added --> 
	<xs:element name="set_dblookup" type="work:set_dblookupType"/><!-- swc20160518 added --> 
	<xs:element name="delete_dblookup" type="work:delete_dblookupType"/><!-- swc20160518 added --> 
 
    <xs:complexType name="delete_dblookupType"><!-- swc20160518 added --><!-- swc20160606 updated -->
        <xs:attribute name="project_path" type="xs:string" use="required" />
        <xs:attribute name="domain_id" type="xs:string" use="required" />
        <xs:attribute name="owner_id" type="xs:string" use="required" />
    </xs:complexType>
 
	<xs:complexType name="set_dblookupType"><!-- swc20160518 added, 20160606 updated -->
		<xs:sequence>
			<xs:element name="domain_id" type="xs:string"/>
		 	<xs:element name="owner_id" type="xs:string"/>
			<xs:element name="db_fullschema" type="xs:string"/>
			<xs:element name="db_datasource" type="xs:string"/>
			<xs:element name="db_servertype" type="xs:string"/>
			<xs:element name="db_nicename" type="xs:string"/>
			<xs:element name="db_tooltip" type="xs:string"/>
			<xs:element name="comment" type="xs:string"/>
			<xs:element name="status_cd" type="xs:string"/>
		</xs:sequence>
		<xs:attribute name="project_path" type="xs:string" use="required" />
	</xs:complexType>
		
    <xs:complexType name="get_dblookupType"><!-- swc20160518 added, 20160606 updated -->
        <xs:attribute name="field" default="project_path">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="project_path"/>
                    <xs:enumeration value="domain_id"/>
                    <xs:enumeration value="owner_id"/>
                    <xs:enumeration value="db_fullschema"/>
                    <xs:enumeration value="db_datasource"/>
                    <xs:enumeration value="db_servertype"/>
                    <xs:enumeration value="db_nicename"/>
                    <xs:enumeration value="db_tooltip"/>
                    <xs:enumeration value="status_cd"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="value" type="xs:string" use="required" />
    </xs:complexType>    
		
    <xs:complexType name="get_dblookupsType"><!-- swc20160518 added -->
        <xs:attribute name="type" default="default">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="default"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>    
		    
    <xs:complexType name="get_childrenType">
        <xs:sequence>
            <xs:element name="parent" type="xs:string"/>
        </xs:sequence>
        <xs:attributeGroup ref="work:return_attributeGroup"/>
    </xs:complexType>
    
    <xs:attributeGroup name="return_attributeGroup">
        <xs:attribute name="hiddens" type="xs:boolean" default="false"/>
        <xs:attribute name="synonyms" type="xs:boolean" default="false"/>
        <xs:attribute name="max" type="xs:int"/>
        <xs:attribute name="type" default="default">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="default"/>
                    <xs:enumeration value="core"/>
                    <xs:enumeration value="all"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="blob" type="xs:boolean" default="false"/>
    </xs:attributeGroup>
    
    <xs:complexType name="get_returnType">
        <xs:attribute name="type" default="default">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="default"/>
                    <xs:enumeration value="core"/>
                    <xs:enumeration value="all"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="blob" type="xs:boolean" default="false"/>
    </xs:complexType>
    
    <xs:complexType name="delete_childType">
        <xs:sequence>
            <xs:element name="node" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="childType">
        <xs:sequence>
            <xs:element name="node" type="xs:string"/>
            <xs:element name="parent" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="rename_childType">
        <xs:sequence>
            <xs:element name="node" type="xs:string"/>
            <xs:element name="name" type="xs:string"/>
            <xs:element name="work_xml" type="work:xml_valueType" /> 
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="export_childType">
            <xs:attribute name="type" default="xs:string"/>
            <xs:attribute name="node" type="xs:string"/>
    </xs:complexType>
    
    <xs:complexType name="annotate_childType">
        <xs:sequence>
            <xs:element name="node" type="xs:string"/>
            <xs:element name="tooltip" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    
        <xs:complexType name="xml_valueType" mixed="false">
        <xs:sequence>
            <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##any" processContents="skip"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="findBy_childType">
        <xs:sequence>
            <xs:element name="match_str" type="work:match_strType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="category" type="xs:string" use="required"/>
        <xs:attributeGroup ref="work:return_attributeGroupWithoutSynonyms"/>
    </xs:complexType>
    
    <xs:attributeGroup name="return_attributeGroupWithoutSynonyms">
        <xs:attribute name="hiddens" type="xs:boolean" default="false"/>
        <xs:attribute name="max">
	 		<xs:simpleType>
	   			<xs:restriction base="xs:int">
      					<xs:minInclusive value="1"/>
    			</xs:restriction>
	  		</xs:simpleType>
		</xs:attribute>
        <xs:attribute name="type" default="core">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="core"/>
                    <xs:enumeration value="all"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="blob" type="xs:boolean" default="true"/>
    </xs:attributeGroup>
    
        <xs:complexType name="match_strType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="strategy" use="required">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="exact"/>
                            <xs:enumeration value="left"/>
                            <xs:enumeration value="right"/>
                            <xs:enumeration value="contains"/>
                           
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    
    <xs:complexType name="protectedType">
  		<xs:sequence>
            <xs:element name="index" type="xs:string" minOccurs="1" maxOccurs="1"/>
            <xs:element name="protectedAccess" type="xs:string" />
        </xs:sequence>
    </xs:complexType>
</xs:schema>
