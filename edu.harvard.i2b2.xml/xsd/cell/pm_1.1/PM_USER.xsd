		<xs:complexType name="configureType"> <xs:sequence> <xs:element
		name="environment" type="xs:string"/> <xs:element name="helpURL"
		type="xs:string"/> <xs:element name="user" type="pm:userType"/>
		<xs:element name="cell_datas" type="pm:cell_datasType"/> <xs:element
		name="global_data" type="pm:global_dataType"/> </xs:sequence>
		</xs:complexType>
	-->
	
	<xs:complexType name="blobType" mixed="true">
		<xs:annotation>
			<xs:documentation>
				A string with all whitespace preserved. This string
				could also contain xml so the type is anyType in order
				for complex element content to be valid.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:restriction base="xs:anyType">
				<xs:sequence>
					<xs:any minOccurs="0" maxOccurs="unbounded" />
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>	
	
	<xs:complexType name="userType">
		<xs:sequence>
			<xs:element name="full_name" type="xs:string" />
			<xs:element name="user_name" type="xs:string" />
			<xs:element name="email" type="xs:string" minOccurs="0" />
			<xs:element name="password" type="pm:passwordType" />
			<xs:element name="domain" type="xs:string" />
			<xs:element name="is_admin" type="xs:boolean" />
			<xs:element name="key" type="xs:string" minOccurs="0" />
			<xs:element name="param" type="pm:paramType" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="project" type="pm:projectType"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="passwordType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="is_token" type="xs:boolean"
					default="false" />
				<xs:attribute name="token_ms_timeout" type="xs:int" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="searchType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="by" type="xs:string" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>	
